{
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.lineHeight": 1.2,
  "files.autoSave": "afterDelay",
  "telemetry.telemetryLevel": "off",
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.showcmd": true,
  "vim.smartRelativeLine": true,
  "vim.sneak": true,
  "vim.surround": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        ".",
        "k"
      ],
      "commands": [
        "workbench.action.openGlobalKeybindings"
      ]
    },
    {
      "before": [
        "<leader>",
        ".",
        "s"
      ],
      "commands": [
        "workbench.action.openSettings",
        "settings.switchToJSON"
      ]
    },
    {
      "before": [
        "<leader>",
        "b",
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "d",
      ],
      "commands": [
        ":noh"
      ]
    },
    {
      "before": [
        "<leader>",
        "e",
        "m",
      ],
      "commands": [
        ":noh"
      ]
    },
    {
      "before": [
        "g",
        "t"
      ],
      "commands": [
        "java.test.goToTest"
      ],
      "when": "resourceExtname == '.java'"
    },
    {
      "before": [
        "g",
        "t"
      ],
      "commands": [
        "java.test.goToTestSubject"
      ],
      "when": "resourceExtname == '.java' && resourceFilename =~ /Test/"
    },
    {
      "before": [
        "n"
      ],
      "after": [
        "n",
        "z",
        "z",
        "z",
        "v"
      ]
    },
    {
      "before": [
        "N"
      ],
      "after": [
        "N",
        "z",
        "z",
        "z",
        "v"
      ]
    },
    {
      "before": [
        "<c-d>"
      ],
      "after": [
        "<c-d>",
        "z",
        "z"
      ]
    },
    {
      "before": [
        "<c-u>"
      ],
      "after": [
        "<c-u>",
        "z",
        "z"
      ]
    },
    {
      "before": [
        "<c-h>"
      ],
      "after": [
        "<c-w>",
        "h"
      ]
    },
    {
      "before": [
        "<c-j>"
      ],
      "after": [
        "<c-w>",
        "j"
      ]
    },
    {
      "before": [
        "<c-k>"
      ],
      "after": [
        "<c-w>",
        "k"
      ]
    },
    {
      "before": [
        "<c-l>"
      ],
      "after": [
        "<c-w>",
        "l"
      ]
    },
    {
      "before": [
        "-"
      ],
      "after": [
        "<c-w>",
        "-"
      ]
    },
    {
      "before": [
        "="
      ],
      "after": [
        "<c-w>",
        "+"
      ]
    },
    {
      "before": [
        "_"
      ],
      "after": [
        "<c-w>",
        "<"
      ]
    },
    {
      "before": [
        "+"
      ],
      "after": [
        "<c-w>",
        ">"
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "c"
      ],
      "commands": [
        "workbench.view.scm"
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "d"
      ],
      "commands": [
        "workbench.view.debug"
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "c"
      ],
      "commands": [
        "workbench.view.scm"
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "d"
      ],
      "commands": [
        "workbench.view.debug"
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "g"
      ],
      "commands": [
        "gradleDefaultProjectsView.focus"
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "m"
      ],
      "commands": [
        "mavenProjects.focus"
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "p"
      ],
      "commands": [
        "workbench.view.explorer"
      ]
    },
    {
      "before": [
        "<leader>",
        "e",
        "V"
      ],
      "commands": [
        "workbench.action.openSettings",
        "settings.switchToJSON"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "p"
      ],
      "commands": [
        "git.pull"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "P"
      ],
      "commands": [
        "git.push"
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "s"
      ],
      "commands": [
        "outline.focus"
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.toggleTerminal"
      ]
    },
    {
      "before": [
        "<leader>",
        "f",
        "o"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "p"
      ],
      "commands": [
        "git.pull"
      ]
    },
    {
      "before": [
        "<leader>",
        "h"
      ],
      "commands": [
        "workbench.action.closeSidebar",
        "workbench.action.closePanel"
      ]
    },
    {
      "before": [
        "<leader>",
        "m"
      ],
      "commands": [
        ":marks"
      ]
    },
    {
      "before": [
        "<leader>",
        "o",
        "i"
      ],
      "commands": [
        "editor.action.organizeImports"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "e"
      ],
      "commands": [
        "editor.action.refactor"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "n"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "r"
      ],
      "commands": [
        "java.debug.runJavaFile"
      ],
      "when": "resourceExtname == .java"
    },
    {
      "before": [
        "<leader>",
        "r",
        "w"
      ],
      "commands": [
        "workbench.action.replaceInFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "a"
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "t"
      ],
      "commands": [
        "workbench.action.findInFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "<leader>"
      ],
      "after": [
        "<c-^>"
      ]
    },
    {
      "before": [
        "<Tab>"
      ],
      "after": [
        "g",
        "t"
      ]
    },
    {
      "before": [
        "<s-Tab>"
      ],
      "after": [
        "g",
        "T"
      ]
    },
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    },
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "J"
      ],
      "after": [
        "}"
      ]
    },
    {
      "before": [
        "H"
      ],
      "after": [
        "{"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    },
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "J"
      ],
      "after": [
        "}"
      ]
    },
    {
      "before": [
        "H"
      ],
      "after": [
        "{"
      ]
    },
    {
      "before": [
        "<leader>",
        "e",
        "m"
      ],
      "commands": [
        {
          "command": "editor.action.codeAction",
          "args": {
            "kind": "refactor.extract.function"
          }
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "e",
        "p"
      ],
      "commands": [
        {
          "command": "editor.action.codeAction",
          "args": {
            "kind": "refactor.introduce"
          }
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "e",
        "v"
      ],
      "commands": [
        {
          "command": "editor.action.codeAction",
          "args": {
            "kind": "refactor.extract.variable"
          }
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "e"
      ],
      "commands": [
        "editor.action.refactor"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "w"
      ],
      "commands": [
        "workbench.action.replaceInFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "t"
      ],
      "commands": [
        "workbench.action.findInFiles"
      ]
    },
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-c>": true,
    "<C-e>": false,
    "<C-r>": true,
    "<C-v>": true,
    "<C-x>": false
  },
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "workbench.colorTheme": "Kimbie Dark",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "java.compile.nullAnalysis.nonnull": [],
  "java.compile.nullAnalysis.nullable": [],
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml"
}
